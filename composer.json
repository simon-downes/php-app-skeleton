{
    "name":        "simon-downes/php-app-skeleton",
    "description": "A PHP webapp skeleton based on Slim v4",
    "keywords":    ["php"],
    "license":     "MIT",
    "authors": [
        {
            "name":     "Simon Downes",
            "email":    "simon@simondownes.co.uk",
            "homepage": "https://simondownes.co.uk",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.4",
        "slim/slim": "4.*",
        "simon-downes/spf-core": "dev-master",
        "simon-downes/spf-support": "dev-master",
        "php-di/php-di": "^6.3",
        "slim/twig-view": "^3.2",
        "symfony/dotenv": "^5.2",
        "slim/psr7": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "spf\\web\\": "lib/spf-web/"
        }
    }
}
